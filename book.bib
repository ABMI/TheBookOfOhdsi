url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/0470013192.bsa606},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/0470013192.bsa606},
year = {2005},
keywords = {confounding, hidden bias, nonrandomized experiment, observational study, sensitivity analysis, treatment effects, unobserved bias},
abstract = {Abstract In an observational study, subjects are not randomly assigned to treatment or control, as they would be in a controlled clinical trial. As a consequence, differing outcomes in treated and control groups may not reflect effects of the treatment, but rather some way the groups were not comparable prior to treatment, that is, some hidden bias. A sensitivity analysis appraises how the conclusions of a study might be altered by hidden biases of various magnitudes.}
}



